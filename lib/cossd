#! /usr/bin/env node
console.log('Starting Node');
console.log('Setting Node timer');
let i = 0;
setInterval(() => {
    console.log('Node is alive', ++i);
}, 1000);
const coss = require('bindings')('coss.node');
console.log('Have COSS', coss);
// FIXME: Somehow specify argv explicitly.
const instance = coss.start((str) => str.toUppercase(), ["hello", "version", "--long"]);
console.log(coss.dispatch(instance, "Hello from node proper!"));
